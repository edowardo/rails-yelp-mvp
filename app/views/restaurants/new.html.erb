<h1>Atarashii Resutoran wo Tsuika Shimasho!</h1>

<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :category, collection: Restaurant::CATEGORIES  %>
  <%= f.input :address %>
  <%= f.input :phone_number%>
  <%= f.submit "Add the Restaurant", class: "btn btn-primary" %>
<% end %>
